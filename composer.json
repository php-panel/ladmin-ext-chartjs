{
    "name": "php-panel/ladmin-ext-chartjs",
    "description": "Use Chartjs in laravel-admin",
    "type": "library",
    "keywords": [
        "laravel-admin",
        "extension",
        "chartjs"
    ],
    "homepage": "https://github.com/laravel-admin-ext/chartjs",
    "license": "MIT",
    "authors": [
        {
            "name": "leeqvip",
            "email": "leeqvip@gmail.com"
        }
    ],
    "require": {
        "php": ">=8.2",
        "php-panel/laravel-admin": "*"
    },
    "require-dev": {
        "phpunit/phpunit": "^11.0"
    },
    "autoload": {
        "psr-4": {
            "Ladmin\\Chartjs\\": "src/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Ladmin\\Chartjs\\ChartjsServiceProvider"
            ]
        }
    }
}